{
  "name": "graphql",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo",
    "dev": "nodemon --watch src --exec ts-node --esm src/server.ts"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/server.ts",
    "./*": [
      "./src/*.ts",
      "./src/*/server.ts"
    ]
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/server"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "eslint": "^8.11.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.3",
    "graphql-yoga": "^3.6.0",
    "ws": "^8.12.1"
  }
}
